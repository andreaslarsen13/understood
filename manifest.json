{
   "author": "A, A",
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon128.png"
   },
   "content_scripts": [ {
      "js": [ 
         "packages/jquery-3.1.0.min.js", 
         "DomParser.js",
         "StorageManager.js",
         "content.js"
      ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Medium inspired floating Popup for selected text to enhance productivity.",
   "homepage_url": "http://andreaslarsen.github.io ",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Understood",
   "permissions": [ "activeTab", "\u003Call_urls>", "tabs", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0",
   "web_accessible_resources": [ "images/*.png" ]
}
